import "./styles.css";
import {useState} from 'react'

export default function Exemple() {

  const [count, setCount]= useState(0)
  const handeClick =() => {
    setCount(count +1)
  }
  return (
    <div className="App">
      
      count : {count}
<br/>
      <button onClick={handeClick}> button </button>

      
    </div>
  );
}
